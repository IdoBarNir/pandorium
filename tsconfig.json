{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@setups/*": ["./setups/*"]
    },
    "outDir": "./build"
  },
  "include": [
    "./pandorium.ts",
    "./pandorium.utils.ts",
    "./setups/*",
    "setups/setupNext/setupNext.ts",
    "setups/setupVercel/setupVercel.ts"
  ]
}
